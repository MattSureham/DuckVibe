# DevForge Pipeline Configuration
# Add your API keys here

# ===========================
# AI Model Providers
# ===========================

# OpenAI (GPT-4, GPT-3.5)
OPENAI_API_KEY=""
OPENAI_BASE_URL="https://api.openai.com/v1"

# Anthropic (Claude)
ANTHROPIC_API_KEY=""
ANTHROPIC_BASE_URL="https://api.anthropic.com"

# Google (Gemini)
GEMINI_API_KEY=""

# Minimax (Chinese LLM)
MINIMAX_API_KEY=""
MINIMAX_GROUP_ID=""
MINIMAX_BASE_URL="https://api.minimax.chat/v1"

# Moonshot AI (Chinese LLM)
MOONSHOT_API_KEY=""
MOONSHOT_BASE_URL="https://api.moonshot.cn/v1"

# Grok/XAI
GROK_API_KEY=""
XAI_API_KEY=""

# ===========================
# Local Models (Ollama)
# ===========================

# Ollama - Local LLM
OLLAMA_ENABLED="false"
OLLAMA_BASE_URL="http://localhost:11434"
OLLAMA_DEFAULT_MODEL="llama3.2"

# Available local models (comma-separated)
# Examples: llama3.2, mistral, codellama, deepseek-coder, qwen2.5
OLLAMA_MODELS="llama3.2,mistral,codellama"

# ===========================
# Agent Model Assignments
# ===========================

# Assign specific models to each agent
# Options: openai, anthropic, gemini, minimax, moonshot, ollama

PM_AGENT_MODEL="anthropic"
PM_AGENT_MODEL_NAME="claude-3-5-sonnet-20241022"

REVERSE_ENGINEER_MODEL="ollama"
REVERSE_ENGINEER_MODEL_NAME="codellama"

DEV_AGENT_MODEL="anthropic"
DEV_AGENT_MODEL_NAME="claude-3-5-sonnet-20241022"

QA_AGENT_MODEL="openai"
QA_AGENT_MODEL_NAME="gpt-4o"

DEPLOY_AGENT_MODEL="openai"
DEPLOY_AGENT_MODEL_NAME="gpt-4o-mini"

# Web Scraper Engineer uses Gemini for cost efficiency
WEB_SCRAPER_MODEL="gemini"
WEB_SCRAPER_MODEL_NAME="gemini-1.5-flash"

# Fallback model if primary fails
FALLBACK_MODEL="ollama"
FALLBACK_MODEL_NAME="llama3.2"

# ===========================
# Search & Data APIs
# ===========================

TAVILY_API_KEY=""
BRAVE_API_KEY=""
PERPLEXITY_API_KEY=""

# ===========================
# Deployment Credentials
# ===========================

DOCKER_USERNAME=""
DOCKER_PASSWORD=""
AWS_ACCESS_KEY_ID=""
AWS_SECRET_ACCESS_KEY=""
AWS_REGION="us-east-1"
KUBECONFIG=""

# ===========================
# Application Settings
# ===========================

NODE_ENV="development"
JWT_SECRET="your-secret-key-change-in-production"
DATABASE_URL="postgresql://user:password@localhost:5432/devforge"

# ===========================
# Pipeline Settings
# ===========================

DEFAULT_PLATFORM="docker"
DEFAULT_ENVIRONMENT="local"
ENABLE_TOURNAMENT="true"

# Debug mode
DEBUG="false"
VERBOSE="false"
